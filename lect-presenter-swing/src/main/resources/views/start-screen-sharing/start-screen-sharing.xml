<?xml version="1.0" encoding="UTF-8"?>
<Panel layout="GridBagLayout" border="EmptyBorder(20, 20, 20, 20)">
	<VBox id="contentContainer">
		<gridbagconstraints gridx="0" gridy="0" insets="0,0,2,0" anchor="GridBagConstraints.WEST" />

		<Label text="start.screen.sharing.windows" border="EmptyBorder(0, 0, 5, 0)" />
		<Panel id="windowContainer" layout="WrapFlowLayout" border="EmptyBorder(5, 20, 20, 20)">
		</Panel>

		<Label text="start.screen.sharing.screens" border="EmptyBorder(0, 0, 5, 0)" />
		<Panel id="screenContainer" layout="WrapFlowLayout" border="EmptyBorder(5, 20, 0, 20)">
		</Panel>
	</VBox>

	<HBox>
		<gridbagconstraints gridx="0" gridy="1" insets="20,0,0,0" weightx="1.0" fill="GridBagConstraints.HORIZONTAL" />
		<box.hglue />
		<Button id="startButton" text="start.screen.sharing.start" />
		<box.hstrut width="5" />
		<Button id="closeButton" text="button.close" />
	</HBox>
</Panel>