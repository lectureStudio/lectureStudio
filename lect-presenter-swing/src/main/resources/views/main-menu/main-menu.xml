<?xml version="1.0" encoding="UTF-8"?>
<menubar>
	<menu id="fileMenu" text="menu.file">
		<menuitem id="openDocumentMenuItem" text="menu.open" accelerator="ctrl O" />
		<menuitem id="closeDocumentMenuItem" text="menu.close" accelerator="ctrl F4" enabled="false" />
		<separator />
		<menuitem id="saveDocumentsMenuItem" text="menu.save.documents" enabled="false" />
		<separator />
		<menuitem id="exitMenuItem" text="menu.exit" accelerator="ctrl Q" />
	</menu>
	<menu text="menu.edit">
		<menuitem id="undoMenuItem" text="menu.undo" accelerator="ctrl Z" enabled="false" />
		<menuitem id="redoMenuItem" text="menu.redo" accelerator="ctrl Y" enabled="false" />
		<separator />
		<menuitem id="settingsMenuItem" text="menu.options" />
	</menu>
	<menu text="menu.view">
		<checkboxmenuitem id="outlineMenuItem" text="menu.contents" />
		<checkboxmenuitem id="fullscreenMenuItem" text="menu.fullscreen" type="checkbox" accelerator="alt ENTER" />
		<menuitem id="customizeToolbarMenuItem" text="menu.toolbar.customize" />
		<menu id="externalWindowsMenu" text="menu.external.windows">
			<checkboxmenuitem id="externalMessagesMenuItem" text="menu.external.messages" />
			<checkboxmenuitem id="externalParticipantsMenuItem" text="menu.external.participants" />
			<checkboxmenuitem id="externalSlidePreviewMenuItem" text="menu.external.slide.preview" />
			<checkboxmenuitem id="externalSpeechMenuItem" text="menu.external.speech" />
			<checkboxmenuitem id="externalNotesMenuItem" text="menu.external.notes" />
			<checkboxmenuitem id="externalSlideNotesMenuItem" text="menu.external.slide.notes" />
		</menu>
		<menu id="messagesPositionMenu" text="menu.messages.position">
			<buttongroup>
				<radiobuttonmenuitem id="messagesPositionLeftMenuItem" text="menu.left" />
				<radiobuttonmenuitem id="messagesPositionBottomMenuItem" text="menu.bottom" />
				<radiobuttonmenuitem id="messagesPositionRightMenuItem" text="menu.right" />
			</buttongroup>
		</menu>
		<menu id="notesPositionMenu" text="menu.notes.position">
			<buttongroup>
				<radiobuttonmenuitem id="notesPositionLeftMenuItem" text="menu.left" />
				<radiobuttonmenuitem id="notesPositionBottomMenuItem" text="menu.bottom" />
			</buttongroup>
		</menu>
		<menu id="slideNotesPositionMenu" text="menu.slide.notes.position">
			<buttongroup>
				<radiobuttonmenuitem id="slideNotesPositionRightMenuItem" text="menu.right" />
				<radiobuttonmenuitem id="slideNotesPositionLeftMenuItem" text="menu.left" />
				<radiobuttonmenuitem id="slideNotesPositionBottomMenuItem" text="menu.bottom" />
				<radiobuttonmenuitem id="slideNotesPositionNoneMenuItem" text="menu.none" />
			</buttongroup>
		</menu>
		<menu id="participantsPositionMenu" text="menu.participants.position">
			<buttongroup>
				<radiobuttonmenuitem id="participantsPositionLeftMenuItem" text="menu.left" />
				<radiobuttonmenuitem id="participantsPositionRightMenuItem" text="menu.right" />
			</buttongroup>
		</menu>
		<menu id="previewPositionMenu" text="menu.preview.position">
			<buttongroup>
				<radiobuttonmenuitem id="previewPositionLeftMenuItem" text="menu.left" />
				<radiobuttonmenuitem id="previewPositionRightMenuItem" text="menu.right" />
			</buttongroup>
		</menu>
		<menu id="splitNotesPositionMenu" text="menu.splitNotes.position">
			<buttongroup>
				<radiobuttonmenuitem id="splitNotesPositionRightMenuItem" text="menu.right" />
				<radiobuttonmenuitem id="splitNotesPositionLeftMenuItem" text="menu.left" />
				<radiobuttonmenuitem id="splitNotesPositionNoneMenuItem" text="menu.none" />
			</buttongroup>
		</menu>
	</menu>
	<menu text="menu.whiteboard">
		<menuitem id="newWhiteboardMenuItem" text="menu.whiteboard.new" />
		<separator />
		<menuitem id="newWhiteboardPageMenuItem" text="menu.slide.new" accelerator="F9" enabled="false" />
		<menuitem id="deleteWhiteboardPageMenuItem" text="menu.slide.delete" accelerator="ctrl D" enabled="false" />
		<separator />
		<checkboxmenuitem id="gridMenuItem" text="menu.grid" accelerator="Q" enabled="false" />
	</menu>
	<menu text="menu.presentation">
		<menuitem id="startRecordingMenuItem" text="menu.recording.start" enabled="false" />
		<menuitem id="stopRecordingMenuItem" text="menu.recording.stop" enabled="false" />
		<separator />
		<checkboxmenuitem id="enableStreamMenuItem" text="menu.stream.start" enabled="false" />
		<checkboxmenuitem id="viewStreamMenuItem" text="menu.stream.view" type="checkbox" selected="false" enabled="false" />
		<checkboxmenuitem id="enableStreamMicrophoneMenuItem" text="menu.stream.microphone.start" type="checkbox" selected="true" enabled="false" />
		<checkboxmenuitem id="enableStreamCameraMenuItem" text="menu.stream.camera.start" type="checkbox" selected="true" enabled="false" />
		<separator />
		<checkboxmenuitem id="enableMessengerMenuItem" text="menu.messenger.start" enabled="false" />
<!--		<checkboxmenuitem id="showMessengerWindowMenuItem" text="menu.messenger.window.visible" type="checkbox" selected="true" enabled="false" />-->
		<separator />
		<menuitem id="selectQuizMenuItem" text="menu.quiz.selection" accelerator="Z" enabled="false" />
		<menuitem id="newQuizMenuItem" text="menu.quiz.new" enabled="false" />
		<menuitem id="closeQuizMenuItem" text="menu.quiz.stop" enabled="false" />
		<separator />
		<menuitem id="pauseStopwatchMenuItem" text="menu.stopwatch.pause" accelerator="shift P" enabled="true" />
		<menuitem id="resetStopwatchMenuItem" text="menu.stopwatch.stop" enabled="true" />
		<menuitem id="configStopwatchMenuItem" text="menu.stopwatch.config" enabled="true" />
	</menu>
	<menu id="bookmarksMenu" text="menu.bookmarks">
		<menuitem id="clearBookmarksMenuItem" text="menu.bookmarks.clear" enabled="false" />
		<menuitem id="newBookmarkMenuItem" text="menu.bookmarks.add" accelerator="B" enabled="false" />
		<menuitem id="removeBookmarkMenuItem" text="menu.bookmarks.remove" enabled="false" />
		<menuitem id="newDefaultBookmarkMenuItem" text="menu.bookmarks.add.default" enabled="false" />
		<separator />
		<menuitem id="gotoBookmarkMenuItem" text="menu.bookmarks.goto" accelerator="G" enabled="false" />
		<menuitem id="previousBookmarkMenuItem" text="menu.bookmarks.previous" accelerator="L" enabled="false" />
		<separator />
		<menuitem id="prevBookmarkMenuItem" text="menu.bookmarks.prev" enabled="false" />
		<menuitem id="nextBookmarkMenuItem" text="menu.bookmarks.next" enabled="false" />
	</menu>
	<menu text="menu.info">
		<menuitem id="logMenuItem" text="menu.log" />
		<menuitem id="aboutMenuItem" text="menu.about" />
	</menu>
	<box.hglue />
	<button id="stopwatchMenu" icon="stopwatch.svg, 20" disabledicon="stopwatch.svg, 20" enabled="true" />
	<box.hstrut width="10" />
	<menu id="timeMenu" icon="clock-indicator.svg, 20" disabledicon="clock-indicator.svg, 20" enabled="false" />
	<box.hstrut width="10" />
	<menu id="recordIndicatorMenu" icon="record-indicator.svg, 20" disabledicon="record-indicator.svg, 20" enabled="false" />
	<menu id="quizIndicatorMenu" icon="quiz-indicator.svg, 20" disabledicon="quiz-indicator.svg, 20" enabled="false" />
	<menu id="messengerIndicatorMenu" icon="messenger-indicator.svg, 20" disabledicon="messenger-indicator.svg, 20" enabled="false" />
	<menu id="speechIndicatorMenu" icon="speech-indicator.svg, 20" disabledicon="speech-indicator.svg, 20" enabled="false" />
	<menu id="streamIndicatorMenu" icon="stream-indicator.svg, 20" disabledicon="stream-indicator.svg, 20" enabled="false" />
</menubar>
